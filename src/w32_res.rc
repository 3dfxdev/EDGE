#define MAIN_MENU            100
#define MAIN_MENU_FILE_EXIT  1001

#define MAIN_ICON            1

MAIN_MENU MENU DISCARDABLE
{
  POPUP "File"
  {
    MENUITEM "Exit", MAIN_MENU_FILE_EXIT
  }
}

MAIN_ICON ICON "data/icon128x128.ico"

1 VERSIONINFO
FILEVERSION 4, 1, 35, 6
PRODUCTVERSION 4, 1, 35, 6
FILEFLAGSMASK 0x0
FILEOS 0x40004 // VOS_NT_WINDOWS32
FILETYPE 1     // VFT_APP
FILESUBTYPE 0  // VFT_UNKNOWN
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    // 0409 means "US English"; 04B0 means "Unicode encoding"
    BLOCK "040904B0"
    BEGIN
      VALUE "CompanyName",      "The EDGE2 Team\0"
      VALUE "FileDescription",  "hyper3DGE\0"
      VALUE "FileVersion",      "1.36.3a\0"
      VALUE "InternalName",     "EDGE136\0"
      VALUE "LegalCopyright",   "\251 Hyper3DGE, GNU General Public License\0"
      VALUE "OriginalFilename", "3DGE136.EXE\0"
      VALUE "ProductName",      "EDGE2\0"
      VALUE "ProductVersion",   "1.36.3a\0"
    END
  END
  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x0409, 0x04B0
  END
END

